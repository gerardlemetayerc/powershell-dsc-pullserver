{
  "database": {
    "name": "dsc",
    "driver": "sqlite3",
    "file": "dsc.db"
  },
  "dsc_pullserver": {
    "cert_file": "dsc-dev.local.crt",
    "enable_https": true,
    "enable_client_cert_validation": false,
    "bypass_ca_validation": true,
    "key_file": "dsc-dev.local.key",
    "port": 8484,
    "registrationKey": "AnyString"
  },
  "web_ui": {
    "cert_file": "dsc-dev.local.crt",
    "enable_https": true,
    "key_file": "dsc-dev.local.key",
    "shared_secret": "example-shared-secret",
    "port": 443
  },
  "saml": {
    "enabled": false
  }
}